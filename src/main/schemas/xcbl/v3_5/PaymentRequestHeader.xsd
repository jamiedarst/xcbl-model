        <xsd:documentation xml:lang="en">
            XML Common Business Library 3.5
            Copyright 2000 Commerce One, Inc.
            Permission is granted to use, copy, modify and distribute the
            DTD's, schemas and modules in the Commerce One XML Common Business
            Library Version 3.5 subject to the terms and conditions specified
            at http://www.xcbl.org/license.html
        </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType name="PaymentRequestHeader">
        <xsd:sequence>
            <xsd:element name="PaymentRequestPurpose">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element ref="Purpose"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="PaymentRequestIssueDate" type="xcblDatetime"/>
            <xsd:element name="PaymentRequestID">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element ref="Reference"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element ref="PayerParty"/>
            <xsd:element ref="Language"/>
            <xsd:element minOccurs="0" name="GeneralNotes" type="xsd:string"/>
            <xsd:element minOccurs="0" name="PaymentRequestAttachment">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element ref="Attachment"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="PaymentRequestHeader" type="PaymentRequestHeader"/>
</xsd:schema>
